<!DOCTYPE html><h3><TMPL_VAR REPORTNAME></h3>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <title><TMPL_VAR REPORTNAME></title>
    <link href = "master.css" rel = "stylesheet">
</head>
<body>
<TMPL_VAR TODAY><hr>
<table width="85%" class="report">
  <tr class="header" bgcolor='#D5D6DE'>
    <th>Payee</th>
    <th>2026</th>
    <th>2025</th>
    <th>2024</th>
    <th>2023</th>
    <th>2022</th>
    <th>All Time</th>
  </tr>

  <TMPL_LOOP NAME=CONTENTS>
    <TMPL_IF __ODD__>
      <tr>
    <TMPL_ELSE>
      <tr bgcolor="#E1EDFB">
    </TMPL_IF>
    
    
      <td><TMPL_VAR NAME=Payee></td>
      <td class="money"><TMPL_VAR NAME=Y1></td>
      <td class="money"><TMPL_VAR NAME=Y2></td>
      <td class="money"><TMPL_VAR NAME=Y3></td>
      <td class="money"><TMPL_VAR NAME=Y4></td>
      <td class="money"><TMPL_VAR NAME=Y5></td>
      <td class="money"><TMPL_VAR NAME=TOTAL></td>
      
    </tr>
  </TMPL_LOOP>
</table>
<TMPL_LOOP ERRORS>
    <hr>
    <TMPL_VAR ERROR>
</TMPL_LOOP>
</body>
<script>
    <!-- Format double to base currency -->
    function currency(n) {
        n = parseFloat(n);
        n =  isNaN(n) ? "" : n.toFixed(2);
        var out = n.toString().replace(".", "|");
        out = out.toString().replace(/\B(?=(\d{3})+(?!\d))/g, "<TMPL_VAR GROUP_SEPARATOR>");
        out = out.replace("|", "<TMPL_VAR DECIMAL_POINT>");
        return out;
    }
    var elements= document.getElementsByClassName("money");
    for (var i = 0; i < elements.length; i++) {
        var element = elements[i];
        element.style.textAlign='right';
        if (element.innerHTML.indexOf("-") > -1) {
            element.style.color="#ff0000";
        } 
       if (element.innerHTML == "") {
}
else {
	element.innerHTML = '<TMPL_VAR PFX_SYMBOL>' + currency(element.innerHTML) +'<TMPL_VAR SFX_SYMBOL>';
}
    }
</script>